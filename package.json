{
  "name": "@kitani/eslint-config",
  "version": "0.0.0-development",
  "description": "Eslint + Prettier config for React (Next.js)",
  "main": "index.js",
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "lib",
    ".prettierrc"
  ],
  "scripts": {
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --fix",
    "semantic-release": "semantic-release --branches main",
    "commit": "git-cz"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "eslint": "7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.5.1"
  },
  "keywords": [
    "config",
    "ecmascript",
    "eslint",
    "eslintconfig",
    "javascript",
    "lint",
    "nextjs",
    "prettier",
    "kitani"
  ],
  "author": "Kitani Islam",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KitaniIslam/eslint-config-kitani.git"
  },
  "bugs": {
    "url": "https://github.com/KitaniIslam/eslint-config-kitani/issues"
  },
  "homepage": "https://github.com/KitaniIslam/eslint-config-kitani#readme",
  "devDependencies": {
    "cz-conventional-changelog": "^3.3.0",
    "semantic-release": "^19.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
